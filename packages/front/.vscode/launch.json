{
    "version": "0.2.0",
    "configurations": [
      {
        "type": "firefox",
        "request": "launch",
        "name": "vuejs: firefox",
        "url": "http://localhost:8080",
        "webRoot": "${workspaceFolder}/src",
        "pathMappings": [
          {
            "url": "http://localhost:8080/src/App.vue",
            "path": "${workspaceFolder}/src/App.vue"
          },
        ]
      }
    ]
  }