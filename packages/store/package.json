{
  "name": "@briefly/store",
  "version": "1.0.0",
  "repository": "https://github.com/AsyncPlanningPoker/BrieflyPlanningPoker",
  "license": "GPL-3.0 License",
  "private": true,
  "packageManager": "yarn@3.2.0",
  "main": "dist/index",
  "types": "dist/index",
  "scripts": {
    "start": "concurrently -k \"tsc -b -w\" \"nodemon -L dist/src/index.js\"",
    "migration:up": "npx knex migrate:up",
    "migration:down": "npx knex migrate:down",
    "seed:up": "npx knex seed:run",
    "build": "tsc -b"
  },
  "dependencies": {
    "@types/node": "^17.0.22",
    "concurrently": "^7.0.0",
    "dotenv": "^16.0.0",
    "knex": "^1.0.4",
    "nodemon": "^2.0.15",
    "pg": "^8.7.3"
  },
  "devDependencies": {
    "ts-node": "^10.7.0",
    "typescript": "^4.6.2"
  }
}
